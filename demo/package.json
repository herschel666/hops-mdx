{
  "name": "hops-mdx-demo",
  "version": "0.1.0",
  "main": "app.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "hops start"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "hops": {
    "mdx": {
      "mdPlugins": [
        ["remark-emoji", { "padSpaceAfter": true }]
      ]
    }
  },
  "jest": {
    "preset": "jest-preset-hops",
    "transform": {
      "\\.mdx?$": "hops-mdx-jest"
    }
  },
  "dependencies": {
    "hops": "^11.0.1",
    "hops-mdx": "^0.1.0",
    "hops-mdx-jest": "^0.1.0",
    "jest": "^23.4.2",
    "jest-preset-hops": "^11.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.6.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.2",
    "remark-emoji": "^2.0.2"
  }
}
